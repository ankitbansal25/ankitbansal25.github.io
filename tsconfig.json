{
    "compilerOptions": {
      "jsx": "react",
      "module": "commonjs",
      "noImplicitAny": true,
      "outDir": "./dist/",
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "target": "es5",
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "esModuleInterop": true,
    },
    "include": [
      "index.tsx",
      "index.d.ts"
    ]
}